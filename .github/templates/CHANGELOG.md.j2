%}{{
          "\n### %s\n" | format(type_ | title)

}}{%      for commit in commits
%}{{
          "\n* %s ([`%s`](%s))\n" | format(
              commit.descriptions[0].rstrip(),
              commit.short_hash,
              commit.hexsha | commit_hash_url
          )

}}{%      endfor
%}{%    endfor
%}{%  endfor
%}
